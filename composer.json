{
    "name": "sgkirby/commentions",
    "description": "Receive and display comments and webmentions",
    "version": "1.0.0-alpha.1",
    "type": "kirby-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastian Greger",
            "email": "msg@sebastiangreger.net"
        }
    ],
    "keywords": [
        "kirby3",
        "kirby3-cms",
        "kirby3-plugin"
    ],
    "require": {
        "getkirby/composer-installer": "^1.1",
        "masterminds/html5": "^2.7",
        "mf2/mf2": "^0.4.6",
        "ezyang/htmlpurifier": "^4.12"
    },
    "autoload": {
        "psr-4": {
            "sgkirby\\Commentions\\": "classes/"
        },
